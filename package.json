{
  "name": "kursaha-sdk",
  "version": "1.0.7",
  "description": "Nodejs library for Kursaha",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "peerDependencies": {
    "react": ">=14"
  },
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint",
    "test": "jest --config jestconfig.json",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "lint:fix": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\" --fix",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\""
  },
  "keywords": [
    "react",
    "typescript",
    "kursaha",
    "customer-engagement-platform"
  ],
  "author": "hello@kursha.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.3.4",
    "axios-retry": "^3.4.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/mocha": "^10.0.1",
    "@types/react": "^18.0.31",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kursaha/node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/kursaha/node-sdk/issues"
  },
  "homepage": "https://github.com/kursaha/node-sdk#readme"
}
